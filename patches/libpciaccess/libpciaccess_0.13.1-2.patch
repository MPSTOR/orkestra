--- libpciaccess-0.13.1.orig/autogen.sh
+++ libpciaccess-0.13.1/autogen.sh
@@ -0,0 +1,12 @@
+#! /bin/sh
+
+srcdir=`dirname $0`
+test -z "$srcdir" && srcdir=.
+
+ORIGDIR=`pwd`
+cd $srcdir
+
+autoreconf -v --install || exit 1
+cd $ORIGDIR || exit $?
+
+$srcdir/configure --enable-maintainer-mode "$@"
--- libpciaccess-0.13.1.orig/debian/rules
+++ libpciaccess-0.13.1/debian/rules
@@ -0,0 +1,21 @@
+#!/usr/bin/make -f
+
+PACKAGE = libpciaccess0
+
+CFLAGS = $(shell dpkg-buildflags --get CFLAGS)
+
+%:
+	dh $@ --with autoreconf,quilt --builddirectory=build/ --parallel
+
+override_dh_auto_configure:
+	dh_auto_configure -- \
+		--with-pciids-path=\$${prefix}/share/misc \
+		--with-zlib \
+		CFLAGS="$(CFLAGS)"
+
+override_dh_install:
+	find debian/tmp -type f -name *.la -delete
+	dh_install --fail-missing
+
+override_dh_makeshlibs:
+	dh_makeshlibs -V'libpciaccess0 (>= 0.12.902)' --add-udeb=$(PACKAGE)-udeb -- -c4
